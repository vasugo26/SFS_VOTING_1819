<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">
    <RelativeLayout
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/activity_result_page"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        tools:context="com.example.vasu.sfs_v_17_18.result_page"
        android:background="@android:color/holo_orange_light">

        <Button
            android:text="CLEAR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:id="@+id/gandhi_clear"
            style="@style/Widget.AppCompat.Button"
            android:textSize="18sp"
            android:layout_below="@+id/textView"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:text="GANDHI HOUSE "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView"
            android:textStyle="normal|bold"
            android:textSize="16sp"

            android:layout_alignParentTop="true"
            android:layout_alignLeft="@+id/textView3"
            android:layout_alignStart="@+id/textView3" />

        <Button
            android:text="CAPTAIN"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tagore_captain"
            android:textSize="18sp"
            android:layout_alignBaseline="@+id/tagore_vice_captain"
            android:layout_alignBottom="@+id/tagore_vice_captain"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <Button
            android:text="CLEAR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:id="@+id/tagore_clear"
            android:textSize="18sp"
            android:layout_below="@+id/textView4"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Button
            android:text="CAPTAIN"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/gandhi_captain"
            style="@style/Widget.AppCompat.Button"
            android:textSize="18sp"
            android:layout_alignBaseline="@+id/gandhi_vice_captain"
            android:layout_alignBottom="@+id/gandhi_vice_captain"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <Button
            android:text="CAPTAIN"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/teresa_captain"
            android:textSize="18sp"
            android:layout_alignBaseline="@+id/teresa_vice_captain"
            android:layout_alignBottom="@+id/teresa_vice_captain"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <Button
            android:text="VICE CAPTAIN"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/gandhi_vice_captain"
            style="@style/Widget.AppCompat.Button"
            android:textSize="18sp"
            android:layout_alignBaseline="@+id/gandhi_clear"
            android:layout_alignBottom="@+id/gandhi_clear"
            android:layout_alignRight="@+id/teresa_vice_captain"
            android:layout_alignEnd="@+id/teresa_vice_captain" />

        <Button
            android:text="VICE CAPTAIN"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/teresa_vice_captain"
            android:textSize="18sp"
            android:fontFamily="sans-serif"

            android:layout_alignBaseline="@+id/teresa_clear"
            android:layout_alignBottom="@+id/teresa_clear"
            android:layout_alignLeft="@+id/textView3"
            android:layout_alignStart="@+id/textView3" />

        <TextView
            android:text="TERESA HOUSE"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            android:textSize="16sp"
            android:textStyle="normal|bold"
            android:layout_marginTop="29dp"

            android:layout_below="@+id/gandhi_vice_captain"
            android:layout_alignLeft="@+id/textView4"
            android:layout_alignStart="@+id/textView4" />

        <Button
            android:text="CLEAR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/teresa_clear"
            android:textSize="18sp"
            android:layout_marginTop="19dp"
            android:layout_below="@+id/textView3"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:text="TAGORE HOUSE"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4"
            android:textSize="16sp"
            android:textStyle="normal|bold"

            android:layout_centerVertical="true"
            android:layout_alignLeft="@+id/tagore_vice_captain"
            android:layout_alignStart="@+id/tagore_vice_captain" />

        <TextView
            android:text="NEHRU HOUSE"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="19dp"
            android:id="@+id/textView5"
            android:textSize="16sp"
            
            android:textStyle="normal|bold"
            android:layout_below="@+id/tagore_vice_captain"
            android:layout_alignLeft="@+id/tagore_vice_captain"
            android:layout_alignStart="@+id/tagore_vice_captain" />

        <Button
            android:text="CLEAR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/nehru_clear"
            android:textSize="18sp"
            android:layout_below="@+id/textView5"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="10dp" />

        <Button
            android:text="CAPTAIN"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/nehru_captain"
            android:textSize="18sp"
            android:layout_alignBaseline="@+id/nehru_vice_captain"
            android:layout_alignBottom="@+id/nehru_vice_captain"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <Button
            android:text="VICE CAPTAIN"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tagore_vice_captain"
            android:textSize="18sp"
            android:layout_alignBaseline="@+id/tagore_clear"
            android:layout_alignBottom="@+id/tagore_clear"
            android:layout_centerHorizontal="true" />

        <Button
            android:text="VICE CAPTAIN"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/nehru_vice_captain"
            android:textSize="18sp"
            android:layout_alignBaseline="@+id/nehru_clear"
            android:layout_alignBottom="@+id/nehru_clear"
            android:layout_alignRight="@+id/teresa_vice_captain"
            android:layout_alignEnd="@+id/teresa_vice_captain" />


    </RelativeLayout>
</ScrollView>