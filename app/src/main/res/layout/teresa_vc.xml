<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    >
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/activity_teresa_vc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        tools:context="com.example.vasu.sfs_v_17_18.teresa_vc"
        android:background="@android:color/holo_purple">

        <Button
            android:id="@+id/MVC4"
            style="@style/Widget.AppCompat.Button.Colored"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/MVC3"
            android:layout_alignParentBottom="true"
            android:layout_alignStart="@+id/MVC3"
            android:layout_marginBottom="33dp"
            android:background="@android:color/holo_blue_dark"
            android:text=" Vartik Chaturvedi "
            android:textAllCaps="false"
            android:textSize="20sp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/nmvc2"
            android:id="@+id/imageView63"
            android:layout_below="@+id/imageView62"
            android:layout_alignLeft="@+id/imageView62"
            android:layout_alignStart="@+id/imageView62"
            android:layout_marginTop="24dp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/nmvc3"
            android:id="@+id/imageView64"
            android:layout_below="@+id/imageView63"
            android:layout_alignLeft="@+id/imageView63"
            android:layout_alignStart="@+id/imageView63"
            android:layout_marginTop="27dp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/nmvc4"
            android:id="@+id/imageView65"
            android:layout_alignParentBottom="true"
            android:layout_alignLeft="@+id/imageView64"
            android:layout_alignStart="@+id/imageView64" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/nmvc1"
            android:id="@+id/imageView62"
            android:layout_marginLeft="24dp"
            android:layout_marginStart="24dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Button
            android:id="@+id/MVC1"
            style="@style/Widget.AppCompat.Button.Colored"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="15dp"
            android:layout_marginTop="47dp"
            android:background="@android:color/holo_blue_dark"
            android:text=" Sohini Ghosh "
            android:textAllCaps="false"
            android:textSize="20sp" />

        <Button
            android:id="@+id/MVC2"
            style="@style/Widget.AppCompat.Button.Colored"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/imageView63"
            android:layout_marginEnd="33dp"
            android:layout_marginRight="33dp"
            android:layout_marginTop="16dp"
            android:background="@android:color/holo_blue_dark"
            android:text=" Aditya Pandey "
            android:textAllCaps="false"
            android:textSize="20sp" />

        <Button
            android:id="@+id/MVC3"
            style="@style/Widget.AppCompat.Button.Colored"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/MVC2"
            android:layout_alignStart="@+id/MVC2"
            android:layout_alignTop="@+id/imageView64"
            android:layout_marginTop="25dp"
            android:background="@android:color/holo_blue_dark"
            android:text=" Nischal Joshi "
            android:textAllCaps="false"
            android:textSize="20sp" />
    </RelativeLayout>
</ScrollView>
